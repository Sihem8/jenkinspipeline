pipeline{
    agent any
    
    stages{
        stage('hello'){
            steps{
                echo'Hello World'
            }
        }
        stage('Build'){
            steps{
                echo'DEV'
            }
        }
        stage('test'){
            steps{
                echo'Testing'
            }
        }
        stage('Deploy'){
            steps{
                echo'Mise en production'
            }
        }
    }
}
